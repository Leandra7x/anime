{% extends 'AnimeDbCatalogBundle::popup.html.twig' %}

{% set popup_id = 'f-refill-popup-' ~ field %}

{% form_theme form 'AnimeDbCatalogBundle:Form:fields.html.twig' %}

{% block content %}
<div class="b-refill-form">
	<form action="" method="post" {{ form_enctype(form) }} data-field="{{ field }}">
		{{ form_widget(form) }}
		<button type="submit">{% trans %}Refill{% endtrans %}</button>
	</form>
</div>
{% endblock %}