{
    "name": "anime-db/anime-db",
    "license": "MIT",
    "type": "project",
    "description": "The application for making home collection anime",
    "homepage": "http://anime-db.org",
    "version": "0.1.8",
    "authors": [
        {
            "name":     "AnimeDB",
            "homepage": "http://anime-db.org"
        },
        {
            "name":     "Peter Gribanov",
            "email":    "info@peter-gribanov.ru"
        },
        {
            "name":     "AnimeDB Community",
            "homepage": "https://github.com/anime-db/anime-db/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-0": { "AnimeDB\\": "src/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/anime-db/worldart-filler-bundle.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-pdo_sqlite": ">=1.0.1",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/buzz-bundle": "dev-master",
        "avalanche123/imagine-bundle": "v2.1",
        "gedmo/doctrine-extensions": "dev-master",
        "knplabs/knp-menu-bundle": "1.1.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "anime-db/worldart-filler-bundle": "0.1.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
