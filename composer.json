{
    "name": "anime-db/anime-db",
    "license": "GPL-3.0",
    "type": "project",
    "description": "The application for making home collection anime",
    "homepage": "http://anime-db.org",
    "version": "0.3.11",
    "authors": [
        {
            "name": "Anime DB",
            "homepage": "http://anime-db.org"
        },
        {
            "name": "Peter Gribanov",
            "email": "info@peter-gribanov.ru"
        },
        {
            "name": "Anime DB Community",
            "homepage": "https://github.com/anime-db/anime-db/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-0": {
            "AnimeDb\\Bundle\\AnimeDbBundle": "src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "doctrine/doctrine-migrations-bundle",
                "type": "symfony-bundle",
                "version": "1.0.1",
                "dist": {
                    "url": "https://github.com/anime-db/DoctrineMigrationsBundle/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/anime-db/DoctrineMigrationsBundle.git",
                    "type": "git",
                    "reference": "refs/heads/master"
                },
                "require": {
                    "php": ">=5.3.2",
                    "symfony/framework-bundle": "~2.1",
                    "doctrine/doctrine-bundle": "~1.0",
                    "doctrine/migrations": "~1.0@dev"
                },
                "autoload": {
                    "psr-0": {
                        "Doctrine\\Bundle\\MigrationsBundle": ""
                    }
                },
                "target-dir": "Doctrine/Bundle/MigrationsBundle"
            }
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-pdo_sqlite": "*",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "gedmo/doctrine-extensions": "2.3.*",
        "doctrine/migrations": "dev-master",
        "doctrine/doctrine-migrations-bundle": "1.0.*",
        "composer/composer": "1.0.*",
        "guzzle/guzzle": "3.7.*",
        "avalanche123/imagine-bundle": "2.*",
        "anime-db/catalog-bundle": "0.4.*"
    },
    "require-dev": {
        "ext-fileinfo": "*"
    },
    "suggest": {
        "ext-apc": "Allows use APC for autoloading to improve performance"
    },
    "scripts": {
        "pre-update-cmd": [
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::backupDB"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::installConfig",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::notifyProjectInstall",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::execJobs",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::migrateUp",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::deliverEvents",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::dumpAssets"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::installConfig",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::notifyProjectUpdate",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::execJobs",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::migrateDown",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::migrateUp",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::deliverEvents",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::dumpAssets"
        ],
        "post-package-install": [
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::addPackageToKernel",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::addPackageToRouting",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::addPackageToConfig",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::migratePackage",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::notifyPackage"
        ],
        "post-package-update": [
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::migratePackage",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::notifyPackage"
        ],
        "pre-package-uninstall": [
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::removePackageFromKernel",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::removePackageFromRouting",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::removePackageFromConfig",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::migratePackage",
            "AnimeDb\\Bundle\\AnimeDbBundle\\Composer\\ScriptHandler::notifyPackage"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    },
    "minimum-stability": "dev"
}
